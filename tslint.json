{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-shadowed-variable": false,
    "member-access": false,
    "ordered-imports":false,
    "no-console": [false, "warning"]
  }
}